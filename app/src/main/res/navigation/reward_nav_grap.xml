<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/reward_nav_grap"
    app:startDestination="@id/rewardCoinHomeFragment">

    <fragment
        android:id="@+id/rewardCoinHomeFragment"
        android:name="com.emedicoz.app.reward.fragment.RewardCoinHomeFragment"
        android:label="@string/mycoins"
        tools:layout="@layout/fragment_reward_coin_home" >
        <action
            android:id="@+id/action_rewardCoinHomeFragment_to_redeemCouponFragment"
            app:destination="@id/redeemCouponFragment"
            />
        <action
            android:id="@+id/action_rewardCoinHomeFragment_to_earnPointFragment"
            app:destination="@id/earnPointFragment" />
    </fragment>
    <fragment
        android:id="@+id/redeemCouponFragment"
        android:name="com.emedicoz.app.reward.fragment.RedeemCouponFragment"
        android:label="@string/mycoins"
        tools:layout="@layout/fragment_redeem_coupon" />
    <fragment
        android:id="@+id/earnPointFragment"
        android:name="com.emedicoz.app.reward.fragment.EarnPointFragment"
        android:label="fragment_earn_point"
        tools:layout="@layout/fragment_earn_point" />

</navigation>