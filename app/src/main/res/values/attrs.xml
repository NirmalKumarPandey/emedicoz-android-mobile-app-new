<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="increaseIcon" format="reference" />
    <attr name="decreaseIcon" format="reference" />
    <attr name="shadowBackground" format="reference" />
    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStylenewtag new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="MaterialProgressBar">
        <attr name="progressColour" format="color" />
        <attr name="secondaryProgressColour" format="color" />
        <attr name="backgroundColour" format="color" />
        <attr name="duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="MetalRecyclerViewPager">
        <attr name="itemMargin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ms9_ShadowContainerView">
        <attr name="ms9_shadowTranslationZ" format="dimension" />
        <attr name="ms9_shadowElevation" format="dimension" />
        <attr name="ms9_spotShadowDrawablesList" format="reference" />
        <attr name="ms9_ambientShadowDrawablesList" format="reference" />
        <attr name="ms9_forceUseCompatShadow" format="boolean" />
        <attr name="ms9_affectsDisplayedPosition" format="boolean" />
        <attr name="ms9_useAmbientShadow" format="boolean" />
        <attr name="ms9_useSpotShadow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="GapFillTextView">
        <attr name="correct_color" format="color" />
        <attr name="wrong_color" format="color" />
        <attr name="gapFillInputStyle"/>
    </declare-styleable>

    <attr name="gapFillInputStyle" format="enum">
        <enum name="PopupWindow" value="0"/>
        <enum name="EditorText" value="1"/>
    </attr>
