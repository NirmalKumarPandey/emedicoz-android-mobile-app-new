<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp50"
        android:orientation="horizontal"
        android:weightSum="2">

        <TextView
            android:id="@+id/reattemptDialogSN"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.35"
            android:paddingTop="@dimen/dp8"
            android:text="1"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="@dimen/sp14" />

        <View
            android:layout_width="@dimen/dp1"
            android:layout_height="@dimen/dp50"
            android:background="@color/greayrefcode_dark" />

        <TextView
            android:id="@+id/reattemptDialogDate"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.6"
            android:paddingTop="@dimen/dp8"
            android:text="10/09/2019"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="@dimen/sp14" />

        <View
            android:layout_width="@dimen/dp1"
            android:layout_height="@dimen/dp50"
            android:background="@color/greayrefcode_dark" />

        <TextView
            android:id="@+id/reattemptDialogTime"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="0.35"
            android:paddingTop="@dimen/dp8"
            android:text="3:30"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="@dimen/sp14" />

        <View
            android:layout_width="@dimen/dp1"
            android:layout_height="@dimen/dp50"
            android:background="@color/greayrefcode_dark" />

        <TextView
            android:id="@+id/reattemptDialogResultDetails"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="@dimen/dp5"
            android:layout_weight="0.7"
            android:background="@drawable/score_bg"
            android:paddingTop="@dimen/dp8"
            android:text="Result"
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="@dimen/sp15" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/greayrefcode_dark" />

